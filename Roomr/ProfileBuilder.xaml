        <Label Text="Create Profile" FontSize="Title" />
        <BoxView Color="LightGray"
HeightRequest="2"
Margin="15, 0, 15, 0"
HorizontalOptions="Fill" />
        <Entry Placeholder="Name" />
        
    </VerticalStackLayout>
</ContentPage>