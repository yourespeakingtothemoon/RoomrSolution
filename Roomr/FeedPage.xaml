<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Roomr.FeedPage"
             Title="Feed Page">
    <ScrollView>
    <VerticalStackLayout>
        <StackLayout x:Name="Cards">
            <StackLayout.GestureRecognizers>
            <SwipeGestureRecognizer Direction="Left" Swiped="SwipeMatch_Swiped"/>
            <SwipeGestureRecognizer Direction="Right" Swiped="SwipeReject_Swiped"/>
        </StackLayout.GestureRecognizers>
        </StackLayout>
        <Label x:Name="test"></Label>
    </VerticalStackLayout>
    </ScrollView>
</ContentPage>